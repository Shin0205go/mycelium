{
  "name": "@mycelium/cli",
  "version": "1.0.0",
  "description": "MYCELIUM CLI - Command-line interface for MYCELIUM RBAC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "mycelium": "./dist/index.js",
    "myc": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "server": "node dist/index.js server",
    "chat": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "dev:server": "npx tsx src/index.ts server",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mycelium",
    "cli",
    "rbac",
    "mcp",
    "agent"
  ],
  "license": "MIT",
  "dependencies": {
    "@mycelium/core": "*",
    "@mycelium/session": "*",
    "@mycelium/shared": "*",
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@inquirer/prompts": "^8.1.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "fastest-levenshtein": "^1.0.16",
    "figures": "^6.1.0",
    "ora": "^8.2.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
